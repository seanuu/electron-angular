{
    "name": "electron-angular",
    "version": "0.0.0",
    "license": "MIT",
    "main": "dist/main/main.js",
    "scripts": {
        "electron:dev": "run-p angular:server electron:watch electron:start",
        "angular:server": "webpack-dev-server --config webpack.angular.dev.config.js --port=4200",
        "electron:watch": "tsc -p src/main/tsconfig.app.json --watch",
        "electron:start": "wait-on dist/main/main.js && electron .",
        "angular:build": "webpack --config webpack.angular.prod.config.js",
        "test": "karma start ./karma.conf.js",
        "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
        "e2e": "protractor ./protractor.conf.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^5.2.0",
        "@angular/common": "^5.2.0",
        "@angular/compiler": "^5.2.0",
        "@angular/core": "^5.2.0",
        "@angular/forms": "^5.2.0",
        "@angular/http": "^5.2.0",
        "@angular/platform-browser": "^5.2.0",
        "@angular/platform-browser-dynamic": "^5.2.0",
        "@angular/router": "^5.2.0",
        "core-js": "^2.4.1",
        "electron": "^1.8.4",
        "immutable": "^3.8.2",
        "jquery": "^3.3.1",
        "lodash": "^4.17.5",
        "rxjs": "^5.5.6",
        "zone.js": "^0.8.19"
    },
    "devDependencies": {
        "@angular-devkit/core": "0.3.2",
        "@angular/cli": "~1.7.4",
        "@angular/compiler-cli": "^5.2.0",
        "@angular/language-service": "^5.2.0",
        "@ngtools/webpack": "1.10.2",
        "@types/jasmine": "~2.8.3",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "autoprefixer": "^7.2.3",
        "circular-dependency-plugin": "^4.2.1",
        "clean-webpack-plugin": "^0.1.19",
        "codelyzer": "^4.0.1",
        "copy-webpack-plugin": "~4.4.1",
        "electron-builder": "^20.9.0",
        "electron-reload": "^1.2.2",
        "file-loader": "^1.1.5",
        "html-webpack-plugin": "^2.29.0",
        "istanbul-instrumenter-loader": "^3.0.0",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "less-loader": "^4.0.5",
        "npm-run-all": "^4.1.2",
        "postcss-import": "^11.0.0",
        "postcss-loader": "^2.0.10",
        "postcss-url": "^7.1.2",
        "protractor": "~5.1.2",
        "raw-loader": "^0.5.1",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.19.1",
        "stylus-loader": "^3.0.1",
        "ts-node": "~4.1.0",
        "tslint": "~5.9.1",
        "typescript": "~2.5.3",
        "uglifyjs-webpack-plugin": "^1.1.8",
        "url-loader": "^0.6.2",
